#!/usr/bin

function create() {
    cd /home/pranin/Projects/createShell/
    python create.py $1
    cd /home/pranin/Projects/MyProjects/$1/
    git init
    git remote add origin git@github.com:shakyapranin/$1.git
    touch README.md
    git add .
    git commit -m "Initial commit"
    git push -u origin master
    code .
}